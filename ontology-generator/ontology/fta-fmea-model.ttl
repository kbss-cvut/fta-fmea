    rdfs:label  "has effects" .

fta-fmea:isManifestedBy
    rdf:type   owl:ObjectProperty ;
    rdfs:label "is manifested by" .

fta-fmea:Mitigation
    rdf:type   owl:Class ;
    rdfs:label "Mitigation" .

fta-fmea:RiskPriorityNumber
    rdf:type   owl:Class ;
    rdfs:label "Risk Priority Number" .

fta-fmea:hasOccurrence
