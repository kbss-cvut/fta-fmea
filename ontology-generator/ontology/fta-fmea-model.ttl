@prefix termit:   <http://onto.fel.cvut.cz/ontologies/application/termit/> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:      <http://www.w3.org/2002/07/owl#> .
@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .
@prefix fta-fmea: <http://onto.fel.cvut.cz/ontologies/fta-fmea-application/> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .

fta-fmea:Component
    rdf:type   owl:Class ;
    rdfs:label "Component" .

fta-fmea:hasFunction
    rdf:type    owl:ObjectProperty ;
    rdfs:label  "has function" ;
    rdfs:domain fta-fmea:Component ;
    rdfs:range  fta-fmea:Function .

fta-fmea:Function
    rdf:type   owl:Class ;
    rdfs:label "Function" .

fta-fmea:hasFailureMode
    rdf:type    owl:ObjectProperty ;
    rdfs:label  "has failure mode" ;
    rdfs:domain fta-fmea:Function ;
    rdfs:range  fta-fmea:FailureMode .

fta-fmea:FailureMode
    rdf:type   owl:Class ;
    rdfs:label "Failure mode" .

fta-fmea:isMitigatedBy
    rdf:type    owl:ObjectProperty ;
    rdfs:label  "is mitigated by" ;
    rdfs:domain fta-fmea:FailureMode ;
    rdfs:range  fta-fmea:Mitigation .

fta-fmea:hasRPN
    rdf:type    owl:ObjectProperty ;
    rdfs:label  "has risk priority number" ;
    rdfs:domain fta-fmea:FailureMode ;
    rdfs:range  fta-fmea:RiskPriorityNumber .

fta-fmea:isCausedBy
    rdf:type    owl:ObjectProperty ;
    rdfs:label  "is caused by" ;
    rdfs:domain fta-fmea:FailureMode ;
    rdfs:range  fta-fmea:Event .

fta-fmea:Mitigation
    rdf:type   owl:Class ;
    rdfs:label "Mitigation" .

fta-fmea:RiskPriorityNumber
    rdf:type   owl:Class ;
    rdfs:label "Risk Priority Number" .

fta-fmea:hasProbability
    rdf:type    owl:DatatypeProperty ;
    rdfs:label  "has probability" ;
    rdfs:domain fta-fmea:RiskPriorityNumber, fta-fmea:FaultEvent ;
    rdfs:range  xsd:float .

fta-fmea:hasSeverity
    rdf:type    owl:DatatypeProperty ;
    rdfs:label  "has severity" ;
    rdfs:domain fta-fmea:RiskPriorityNumber ;
    rdfs:range  xsd:decimal .

fta-fmea:hasDetection
    rdf:type    owl:DatatypeProperty ;
    rdfs:label  "has detection" ;
    rdfs:domain fta-fmea:RiskPriorityNumber ;
    rdfs:range  xsd:decimal .

fta-fmea:Event
    rdf:type   owl:Class ;
    rdfs:label "Event" .

fta-fmea:causes
    rdf:type    owl:ObjectProperty ;
    rdfs:label  "causes" ;
    rdfs:domain fta-fmea:Event ;
    rdfs:range  fta-fmea:FailureMode .

fta-fmea:Gate
    rdf:type        owl:Class ;
    rdfs:label      "Gate" ;
    rdfs:subClassOf fta-fmea:Event .

fta-fmea:hasGateType
    rdf:type    owl:DatatypeProperty ;
    rdfs:label  "has gate type" ;
    rdfs:domain fta-fmea:Gate ;
    rdfs:range  xsd:string .

fta-fmea:FaultEvent
    rdf:type        owl:Class ;
    rdfs:label      "Fault event" ;
    rdfs:subClassOf fta-fmea:Event .

fta-fmea:consistsOf
    rdf:type    owl:ObjectProperty ;
    rdfs:label  "consists of" ;
    rdfs:domain fta-fmea:FaultEvent ;
    rdfs:range  fta-fmea:FaultEvent .

fta-fmea:produces
    rdf:type    owl:ObjectProperty ;
    rdfs:label  "produces" ;
    rdfs:domain fta-fmea:FaultEvent ;
    rdfs:range  fta-fmea:FaultEvent .

fta-fmea:hasFaultEventType
    rdf:type    owl:DatatypeProperty ;
    rdfs:label  "has fault event type" ;
    rdfs:domain fta-fmea:FaultEvent ;
    rdfs:range  xsd:string .

fta-fmea:influences
    rdf:type    owl:ObjectProperty ;
    rdfs:label  "influences" ;
    rdfs:domain fta-fmea:FaultEvent ;
    rdfs:range  fta-fmea:Component .

fta-fmea:isPreventedBy
    rdf:type    owl:ObjectProperty ;
    rdfs:label  "is prevented by" ;
    rdfs:range  fta-fmea:FaultEvent ;
    rdfs:domain fta-fmea:TakenAction .

fta-fmea:TakenAction
    rdf:type   owl:Class ;
    rdfs:label "Taken action" .

fta-fmea:prevents
    rdf:type    owl:ObjectProperty ;
    rdfs:label  "prevents" ;
    rdfs:domain fta-fmea:TakenAction ;
    rdfs:range  fta-fmea:FaultEvent .

fta-fmea:User
    rdf:type   owl:Class ;
    rdfs:label "User" .

fta-fmea:hasUsername
    rdf:type    owl:DatatypeProperty ;
    rdfs:label  "has username" ;
    rdfs:domain fta-fmea:User ;
    rdfs:range  xsd:string .

fta-fmea:hasPassword
    rdf:type    owl:DatatypeProperty ;
    rdfs:label  "has password" ;
    rdfs:domain fta-fmea:User ;
    rdfs:range  xsd:string .

fta-fmea:authoredBy
    rdf:type    owl:ObjectProperty ;
    rdfs:label  "authored by" ;
    rdfs:range  fta-fmea:User .

fta-fmea:hasCreationDate
    rdf:type    owl:ObjectProperty ;
    rdfs:label  "has creation date" ;
    rdfs:domain xsd:date .

fta-fmea:hasName
    rdf:type   owl:DatatypeProperty ;
    rdfs:label "has name" ;
    rdfs:range xsd:string .

fta-fmea:hasDescription
    rdf:type   owl:DatatypeProperty ;
    rdfs:label "has description" ;
    rdfs:range xsd:string .